# Mode test sans authentification
ENVIRONMENT=test
API_HOST=localhost
API_PORT=8000

# Elasticsearch désactivé pour les tests
ELASTIC_HOST=disabled
ELASTIC_PORT=9200
ELASTIC_USER=
ELASTIC_PASSWORD=

# Keycloak désactivé pour les tests
KEYCLOAK_ISSUER=http://localhost:8080/realms/myrealm
KEYCLOAK_CLIENT_ID=test-client
KEYCLOAK_CLIENT_SECRET=test-secret
KEYCLOAK_ALGORITHMS=RS256

# Chemins des modèles
SCORING_MODEL_PATH=app/models/scoring_model.onnx
FRAUDE_MODEL_PATH=app/models/fraude_model.onnx

# Seuils
SCORE_THRESHOLD=0.5
FRAUDE_ALERT_THRESHOLD=0.7

# Logging
LOG_LEVEL=INFO
LOG_INDEX=api-logs-test
SERVICE_NAME=api_scoring_test

# Autres paramètres
MAX_PAYLOAD_SIZE=1048576